<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">			
	<div class="row">
		<ol class="breadcrumb">
			<li>
				<a href="#">
					<svg class="glyph stroked clipboard with paper">
						<use xlink:href="#stroked-clipboard-with-paper"/>
					</svg>
				</a>
			</li>
			<li class="active">Profiles</li>
		</ol>
	</div><!--/.row-->
	<div class="row">
		<div class="col-lg-12">
			<h1 class="page-header">Profiles</h1>
		</div>
	</div><!--/.row-->
	<%= render partial: "shared/header_info"%>
	<div class="row">
		<div class="col-lg-12">
			<div class="panel panel-default">						
				<div class="panel-body">
					<table data-toggle="table">
				    <thead>
					    <tr>
				        <th>Profile Name</th>
				        <th>Frequency</th>
				        <th>File</th>
				        <th>Storage</th>
				        <th>Last Backup</th>
				        <th>Next Backup</th>
				        <th></th>
					    </tr>
				    </thead>
				    <tbody>
				    	<% if @profiles.present? %>
				    		<% @profiles.each do |profile|%>
						    	<tr>
						        <td>Profile Name</td>
						        <td>Frequency</td>
						        <td>File</td>
						        <td>Storage</td>
						        <td>Last Backup</td>
						        <td>Next Backup</td>
						        <td></td>
						    	</tr>				    					    		
				    		<% end %>
				    	<% end %>
				    </tbody>
					</table>
				</div>
			</div>
		</div>
	</div><!--/.row-->	


		
</div>	<!--/.main-->
